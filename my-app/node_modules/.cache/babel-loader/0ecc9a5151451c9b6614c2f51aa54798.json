{"ast":null,"code":"import DS from '../services/DataService'; //import DataTable from 'datatables.net-vue3';\n\nexport default {\n  // components:{\n  //         DataTable\n  //     },\n  name: \"my-home\",\n\n  data() {\n    return {\n      tableData: [{\n        date: '2016-05-03',\n        name: 'Tom',\n        address: 'No. 189, Grove St, Los Angeles'\n      }, {\n        date: '2016-05-02',\n        name: 'Tom',\n        address: 'No. 189, Grove St, Los Angeles'\n      }, {\n        date: '2016-05-04',\n        name: 'Tom',\n        address: 'No. 189, Grove St, Los Angeles'\n      }, {\n        date: '2016-05-01',\n        name: 'Tom',\n        address: 'No. 189, Grove St, Los Angeles'\n      }],\n      getResult: null,\n      getId: \"\",\n      getfName: \"\",\n      getlName: \"\"\n    };\n  },\n\n  methods: {\n    formatResponse(data) {\n      const container = document.getElementById(\"container\");\n      container.innerHTML = \"\";\n      data.forEach(element => {\n        const wrapper = document.createElement(\"div\");\n        Object.values(element).forEach(value => {\n          //const keydiv= document.createElement(\"div\")\n          //keydiv.append(key)\n          wrapper.append(value + \" \");\n        }); //this.getResult\n\n        container.append(wrapper);\n      });\n    },\n\n    newArray(data) {\n      var a = {\n        Students: []\n      };\n      var i = 0;\n      data.forEach(element => {\n        a.Students[i] = element;\n        i = i + 1;\n      });\n      return a;\n    },\n\n    async getAllData() {\n      try {\n        const res = await DS.getAll(); //this.formatRespons  e(res.data)\n\n        console.log(res.data);\n        console.log(this.tableData);\n        this.getResult = this.newArray(res.data);\n      } catch (e) {\n        this.formatResponse(e.getResult);\n      }\n    },\n\n    async getDataById() {\n      let id = Number(this.getId); //console.log(id)\n\n      try {\n        const res = await DS.getById(id);\n        this.formatResponse(res.data);\n      } catch (error) {\n        this.getResult = this.formatResponse(error.getResult);\n      }\n    },\n\n    async searchByName() {\n      const fname = this.getfName;\n      const lname = this.getlName;\n\n      try {\n        const res = await DS.getByName(fname, lname);\n        this.formatResponse(res.data);\n      } catch (error) {\n        this.getResult = this.formatResponse(error.getRsult);\n      }\n    },\n\n    clearGetOutput() {\n      console.log(\"clearing output\");\n      this.getResult = null;\n      const container = document.getElementById(\"container\");\n      container.innerHTML = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0CE,OAAOA,EAAP,MAAe,yBAAf,C,CAEA;;AACA,eAAc;EACZ;EACA;EACA;EACAC,IAAI,EAAE,SAJM;;EAKZC,IAAI,GAAE;IACJ,OAAM;MACJC,SAAS,EAAE,CACjB;QACEC,IAAI,EAAE,YADR;QAEEH,IAAI,EAAE,KAFR;QAGEI,OAAO,EAAE;MAHX,CADiB,EAMjB;QACED,IAAI,EAAE,YADR;QAEEH,IAAI,EAAE,KAFR;QAGEI,OAAO,EAAE;MAHX,CANiB,EAWjB;QACED,IAAI,EAAE,YADR;QAEEH,IAAI,EAAE,KAFR;QAGEI,OAAO,EAAE;MAHX,CAXiB,EAgBjB;QACED,IAAI,EAAE,YADR;QAEEH,IAAI,EAAE,KAFR;QAGEI,OAAO,EAAE;MAHX,CAhBiB,CADP;MAuBJC,SAAS,EAAE,IAvBP;MAwBJC,KAAK,EAAE,EAxBH;MAyBJC,QAAQ,EAAE,EAzBN;MA0BJC,QAAQ,EAAE;IA1BN,CAAN;EA6BD,CAnCW;;EAoCZC,OAAO,EAAC;IACNC,cAAc,CAACT,IAAD,EAAM;MAClB,MAAMU,SAAQ,GAAIC,QAAQ,CAACC,cAAT,CAAwB,WAAxB,CAAlB;MAEAF,SAAS,CAACG,SAAV,GAAoB,EAApB;MACAb,IAAI,CAACc,OAAL,CAAaC,OAAM,IAAK;QAEtB,MAAMC,OAAM,GAAIL,QAAQ,CAACM,aAAT,CAAuB,KAAvB,CAAhB;QAEAC,MAAM,CAACC,MAAP,CAAcJ,OAAd,EAAuBD,OAAvB,CAA+BM,KAAK,IAAE;UACpC;UACA;UACAJ,OAAO,CAACK,MAAR,CAAeD,KAAI,GAAI,GAAvB;QACD,CAJD,EAJsB,CAStB;;QACAV,SAAS,CAACW,MAAV,CAAiBL,OAAjB;MACD,CAXD;IAYD,CAjBK;;IAmBNM,QAAQ,CAACtB,IAAD,EACR;MACE,IAAIuB,IAAI;QAAEC,QAAQ,EAAE;MAAZ,CAAR;MACA,IAAIC,CAAC,GAAC,CAAN;MACAzB,IAAI,CAACc,OAAL,CAAaC,OAAO,IAAE;QACpBQ,CAAC,CAACC,QAAF,CAAWC,CAAX,IAAcV,OAAd;QACAU,CAAC,GAACA,CAAC,GAAC,CAAJ;MACD,CAHD;MAIA,OAAOF,CAAP;IACD,CA5BK;;IAiCN,MAAMG,UAAN,GAAkB;MAClB,IAAI;QACF,MAAMC,GAAG,GAAC,MAAM7B,EAAE,CAAC8B,