lt{\n    name: \"my-home\",\n    data(){\n      return{ \n        getResult: null\n        // getResult:{\n        //   id: null,\n        //   fname: null,\n        //   lname: null,\n        //   username: null,\n        //   email: null,\n        //   password: null\n        // }\n      }\n    },\n    methods:{\n      formatResponse(data){\n        // data = JSON.parse(JSON.stringify(data))\n        // const {id, fname, lname, username, password, email} = data\n        // return {id, fname, lname, username, password, email}\n        return JSON.stringify(data)\n      },\n\n      async getAllData(){\n       try {\n        const res=await DS.getAll()\n                \n        \n\n        this.getResult = this.formatResponse(res.data)\n        // this.getResult.id=res.data.id\n        // this.getResult.username=res.data.username\n        // this.getResult.fname=res.data.fname\n        // this.getResult.lname=res.data.lname\n        // this.getResult.password=res.data.password\n        // this.getResult.email=res.data.email\n       }\n       catch(e){\n        this.getResult = this.formatResponse(e.result)\n       }\n      },\n\n      async getDataById()\n      {\n        const id = this.$refs.get_id.value\n        console.log(id)\n        try {\n          const res = await DS.getById(id)\n          this.getResult=this.formatResponse(res.data)\n        } catch (error) {\n          this.getResult=this.formatResponse(error.result)\n        }\n      }\n    }\n\n\n\n\n  }\n</script>\n\n"]},"metadata":{},"sourceType":"module"}