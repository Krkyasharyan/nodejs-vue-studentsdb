{"ast":null,"code":"import DS from '../services/DataService';\nimport updatest from './student-details';\nexport default {\n  name: \"my-home\",\n\n  data() {\n    return {\n      dynamicComponent: updatest,\n      getResult: null,\n      getId: \"\",\n      getfName: \"\",\n      getlName: \"\"\n    };\n  },\n\n  methods: {\n    async getAllData() {\n      this.clearOutput();\n      const res = await DS.getAll();\n      this.getResult = res.data;\n    },\n\n    async getDataById() {\n      this.clearOutput();\n      let id = Number(this.getId);\n      const res = await DS.getById(id);\n      this.getResult = res.data;\n    },\n\n    async searchByName() {\n      this.clearOutput();\n      const fname = this.getfName;\n      const lname = this.getlName;\n\n      try {\n        const res = await DS.getByName(fname, lname);\n        this.getResult = res.data;\n      } catch (error) {\n        this.getResult = this.formatResponse(error.getRsult);\n      }\n    },\n\n    editStudentO() {},\n\n    clearOutput() {\n      console.log(\"clearing output\");\n      this.getResult = null;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDE,OAAOA,EAAP,MAAe,yBAAf;AACA,OAAOC,QAAP,MAAqB,mBAArB;AACA,eAAc;EAEZC,IAAI,EAAE,SAFM;;EAGZC,IAAI,GAAE;IACJ,OAAM;MACJC,gBAAgB,EAAEH,QADd;MAEJI,SAAS,EAAE,IAFP;MAGJC,KAAK,EAAE,EAHH;MAIJC,QAAQ,EAAE,EAJN;MAKJC,QAAQ,EAAE;IALN,CAAN;EAOD,CAXW;;EAYZC,OAAO,EAAC;IACN,MAAMC,UAAN,GAAkB;MAClB,KAAKC,WAAL;MACA,MAAMC,GAAG,GAAC,MAAMZ,EAAE,CAACa,MAAH,EAAhB;MACA,KAAKR,SAAL,GAAeO,GAAG,CAACT,IAAnB;IACC,CALK;;IAON,MAAMW,WAAN,GACA;MACE,KAAKH,WAAL;MACA,IAAII,EAAC,GAAIC,MAAM,CAAC,KAAKV,KAAN,CAAf;MACA,MAAMM,GAAE,GAAI,MAAMZ,EAAE,CAACiB,OAAH,CAAWF,EAAX,CAAlB;MACA,KAAKV,SAAL,GAAeO,GAAG,CAACT,IAAnB;IAED,CAdK;;IAgBN,MAAMe,YAAN,GACA;MACE,KAAKP,WAAL;MACA,MAAMQ,KAAK,GAAC,KAAKZ,QAAjB;MACA,MAAMa,KAAK,GAAC,KAAKZ,QAAjB;;MACA,IAAI;QACF,MAAMI,GAAG,GAAC,MAAMZ,EAAE,CAACqB,SAAH,CAAaF,KAAb,EAAoBC,KAApB,CAAhB;QACA,KAAKf,SAAL,GAAeO,GAAG,CAACT,IAAnB;MACF,CAHA,CAGE,OAAOmB,KAAP,EAAc;QACd,K