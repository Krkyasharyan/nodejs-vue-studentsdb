    const res=await DS.getAll()\n        console.log(JSON.parse(res.data).username)\n        this.getResult=JSON.parse(res.data).username\n        \n      \n\n       }\n       catch(e){\n        this.getResult = this.formatResponse(e.result)\n       }\n      },\n\n      async getDataById()\n      {\n        const id = this.$refs.get_id.value\n        try {\n          const res = await DS.getById(id)\n          this.result=this.formatResponse(res)\n        } catch (error) {\n          this.result=this.formatResponse(error.result)\n        }\n      }\n    }\n\n\n\n\n  }\n</script>\n\n"]},"metadata":{},"sourceType":"module"}