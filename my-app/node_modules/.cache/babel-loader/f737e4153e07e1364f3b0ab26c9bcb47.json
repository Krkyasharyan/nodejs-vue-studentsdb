button class=\"btn btn-primary\" @click=\"FindByName\">Find by name</button>\n          </div>\n          <button class=\"btn btn-sm btn-warning ml-2\" @click=\"clearGetOutput\">Clear</button>\n\n          <div v-if=\"getResult\" class=\"alert alert-secondary mt-2\" role=\"alert\"><p>{{getResult}}</p></div>\n        </div> \n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport DS from '../services/DataService'\n\n  export default{\n    name: \"my-home\",\n    data(){\n      return{ \n       // getResult: null\n        getResult:{\n          id: null,\n          fname: null,\n          lname: null,\n          username: null,\n          email: null,\n          password: null\n        }\n      }\n    },\n    methods:{\n      formatResponse(data){\n        return JSON.stringify(data)\n      },\n\n      async getAllData(){\n       try {\n        const res=await DS.getAll()\n        console.log(JSON.parse(res.data))\n        //this.getResult=res.body\n        \n        this.getResult.id=JSON.parse(res.body).id\n        this.getResult.username=JSON.parse(res.body).username\n        this.getResult.fname=JSON.parse(res.body).fname\n        this.getResult.lname=JSON.parse(res.body).lname\n        this.getResult.password=JSON.parse(res.body).password\n        this.getResult.email=JSON.parse(res.body).email\n\n       }\n       catch(e){\n        this.getResult = this.formatResponse(e.result)\n       }\n      },\n\n      async getDataById()\n      {\n        const id = this.$refs.get_id.value\n        try {\n          const res = await DS.getById(id)\n          this.result=this.formatResponse(res)\n        } catch (error) {\n          this.result=this.formatResponse(error.result)\n        }\n      }\n    }\n\n\n\n\n  }\n</script>\n\n"]},"metadata":{},"sourceType":"module"}